      { "source": "/adminService", "destination": "/adminService", "statusCode": 200 },
      { "source": "/admin/users/edit", "destination": "/admin/users/edit", "statusCode": 200 },
      { "source": "/cart", "destination": "/cart", "statusCode": 200 }
    ]
  }
  